{
    "builds": [
        {
            "src": "./frontend/package.json",
            "use": "@vercel/node"
        },
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/(.*)",
            "dest": "./frontend/build/$1"
        }
    ]
}